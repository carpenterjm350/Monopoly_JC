<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.monopoly_1.Controller_StartPage">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="-1.0" pickOnBounds="true">
         <image>
            <Image url="@images/background.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="158.0" fitWidth="393.0" layoutX="104.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="play_Button" disable="true" layoutX="413.0" layoutY="200.0" mnemonicParsing="false" onAction="#playButtonClicked" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #b60500; -fx-border-color: black;" text="Play" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Exit_Button" layoutX="413.0" layoutY="329.0" mnemonicParsing="false" onAction="#ExitButtonClicked" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #b60500; -fx-border-color: black;" text="Exit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="Login_Button" layoutX="413.0" layoutY="241.0" mnemonicParsing="false" onAction="#LoginButtonClicked" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #b60500; -fx-border-color: black;" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="SignUp_Button" layoutX="413.0" layoutY="287.0" mnemonicParsing="false" onAction="#SignUpButtonClicked" prefHeight="26.0" prefWidth="140.0" style="-fx-background-color: #b60500; -fx-border-color: black;" text="Create An Account" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
